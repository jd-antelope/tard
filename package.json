{
  "name": "frontend-c-comps",
  "version": "0.0.1",
  "description": "components",
  "main": "index.js",
  "scripts": {
    "dev": "./scripts/start.sh",
    "taro-dev": "cd packages/demo-taro && yarn dev:h5",
    "vite-dev": "vite",
    "build": "vite build --config vite.config.build.ts",
    "build:es": "rimraf dist && NODE_ENV=production rollup -c rollup.config.es.js",
    "build:lib": "tsc --project tsconfig.d.json",
    "dev:lib": "tsc --project tsconfig.d.json --watch --incremental",
    "dev:watch": "NODE_ENV=development rollup --config rollup.config.dev.js --watch",
    "preview": "vite preview"
  },
  "repository": {
    "type": "git",
    "url": "https://coding.jd.com/selling-front/frontend-c-comps.git"
  },
  "keywords": [
    "taro"
  ],
  "author": "hawleyHuo",
  "license": "ISC",
  "dependencies": {
    "antd": "^4.18.7",
    "react-router-dom": "^6.2.1"
  },
  "devDependencies": {
    "@jd-antelope/commitlint-config-selling": "^0.0.3",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@tarojs/components": "^3.4.2",
    "@tarojs/react": "^3.4.2",
    "@tarojs/runtime": "^3.4.2",
    "@tarojs/taro": "^3.4.2",
    "@types/node": "^17.0.18",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^1.0.7",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "classnames": "^2.3.1",
    "eslint-config-selling": "0.0.3",
    "fs-extra": "^10.0.1",
    "globby": "^11.0.4",
    "husky": "^4.2.3",
    "less": "^4.1.2",
    "lint-staged": "^12.3.4",
    "node-watch": "^0.7.3",
    "postcss-pxtorem": "^6.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.68.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-typescript2": "^0.31.2",
    "rollup-plugin-uglify": "^6.0.4",
    "rollup-plugin-watch": "^1.0.1",
    "stylelint-config-selling": "0.0.3",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5",
    "vite": "^2.8.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue}": [
      "eslint --fix"
    ],
    "*.less": [
      "stylelint --config  ./.stylelintrc --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}